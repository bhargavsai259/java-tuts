import java.util.Scanner;
public class Main{
    public static void main(String args[])
    {
        
        String a="Apple";
        
        int i=0;
        
        while(Character.toString(a.charAt(i)).isEmpty())
        {
            if(Character.toString(a.charAt(i)).isEmpty())
        {
            System.out.println("end");
        }
        else
        {
            System.out.println(a.charAt(i));
            i+=1;
        }
        }
        
    }
}
